{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react",
    "sourceMap": true,
    "typeRoots": ["./node_modules/@types", "./typings/**/*"],
    "outDir": "wwwroot/dist",
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "strictNullChecks": true
    
  },
  "exclude": [
      "bin",
      "node_modules"
  ]
}

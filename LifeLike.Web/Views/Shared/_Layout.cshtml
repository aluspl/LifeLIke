<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
   <title>@ViewData["Title"] - LifeLike</title>   
    <link href="~/dist/styles/libs.css" rel="stylesheet" />
    @RenderSection("Styles", required: false)
</head>
<body>
<nav class="navbar navbar">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>          
        </div>
        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
                <li><a asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
                <li><a asp-area="" asp-controller="Album" asp-action="Index">Albums</a></li>
                <li><a asp-area="" asp-controller="Page" asp-action="OpenPage">Pages</a></li>
                <li><a asp-area="" asp-controller="Page" asp-action="OpenPost">Posts</a></li>

                <li><a asp-area="" asp-controller="Log" asp-action="Index">Logs</a></li>

            </ul>
        </div>
    </div>
</nav>

<div class="container body-content">
    @RenderBody()
</div>


<script src="~/dist/commons.js"></script>
<script src="~/dist/libs.js"></script>
 
@RenderSection("Scripts", required: false)
</body>
</html>